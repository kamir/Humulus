   <rdfs:label>Stability of community cores in evolving networks</rdfs:label>
   <foaf:maker rdf:resource="http://data.semanticweb.org/person/jean-loup-guillaume"/>
   <foaf:maker rdf:resource="http://data.semanticweb.org/person/massoud-seifi"/>
</swrc:InProceedings>

