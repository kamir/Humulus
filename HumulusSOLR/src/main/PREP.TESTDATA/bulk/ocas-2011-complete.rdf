    <swrc:InProceedings rdf:about="http://data.semanticweb.org/workshop/ocas/2011/paper/6">
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/workshop/ocas/2011/proceedings"/>
        <dc:creator rdf:resource="http://data.semanticweb.org/person/csongor-nyulas"/>
