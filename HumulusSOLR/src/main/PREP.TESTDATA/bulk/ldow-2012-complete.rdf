<rdf:Description rdf:about="http://data.semanticweb.org/workshop/ldow/2012/paper/29">
   <swc:isPartOf rdf:resource="http://data.semanticweb.org/workshop/ldow/2012/proceedings"/>
   <dc:creator rdf:resource="http://data.semanticweb.org/person/giuseppe-rizzo"/>
