<rdf:Description rdf:about="http://data.semanticweb.org/workshop/ldow/2012/paper/14">
   <swc:isPartOf rdf:resource="http://data.semanticweb.org/workshop/ldow/2012/proceedings"/>
   <dc:creator rdf:resource="http://data.semanticweb.org/person/daniele-dellaglio"/>
   <dc:creator rdf:resource="http://data.semanticweb.org/person/emanuele-della-valle"/>
   <dc:creator rdf:resource="http://data.semanticweb.org/person/irene-celino"/>
