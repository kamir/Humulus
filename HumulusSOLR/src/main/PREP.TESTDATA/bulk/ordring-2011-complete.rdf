    <swrc:InProceedings rdf:about="http://data.semanticweb.org/workshop/ordring/2011/paper/1">
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/workshop/ordring/2011/proceedings"/>
        <dc:creator rdf:resource="http://data.semanticweb.org/person/michael-opitz"/>
        <dc:creator rdf:resource="http://data.semanticweb.org/person/thorsten-liebig"/>
        <dc:subject>OWL 2 RL</dc:subject>
        <dc:subject>incremental updates</dc:subject>
        <dc:subject>materialization</dc:subject>
        <dc:subject>stream reasoning</dc:subject>
        <dc:title>Reasoning over Dynamic Data in Expressive Knowledge Bases with Rscale</dc:title>
        <swrc:abstract>We introduce Rscale, a secondary storage-aware OWL 2 RL reasoning system capable of dealing with incremental additions and deletions of facts. Our initial evaluation indicates that Rscale is suitable for stream reasoning scenarios characterized by expressive reasoning tasks triggered by a moderate change frequency.</swrc:abstract>
        <swrc:author rdf:resource="http://data.semanticweb.org/person/michael-opitz"/>
        <swrc:author rdf:resource="http://data.semanticweb.org/person/thorsten-liebig"/>
        <srwc-ext:authorList rdf:resource="http://data.semanticweb.org/workshop/ordring/2011/paper/1/authorlist"/>
        <rdfs:label>Reasoning over Dynamic Data in Expressive Knowledge Bases with Rscale</rdfs:label>
        <maker rdf:resource="http://data.semanticweb.org/person/michael-opitz"/>
        <maker rdf:resource="http://data.semanticweb.org/person/thorsten-liebig"/>
    </swrc:InProceedings>

