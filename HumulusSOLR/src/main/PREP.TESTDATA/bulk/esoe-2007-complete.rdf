            <rdf:_2 rdf:resource="http://data.semanticweb.org/person/aldo-franco-dragoni"/>
        </swrc_ext:authorList>
        <owl:sameAs rdf:resource="http://revyu.com/things/esoe-2007-paper-562-ontology-revision"/>
        <maker rdf:resource="http://data.semanticweb.org/person/aldo-franco-dragoni"/>
