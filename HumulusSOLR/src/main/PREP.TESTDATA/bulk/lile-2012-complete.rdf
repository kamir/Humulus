<rdf:Description rdf:about="http://data.semanticweb.org/workshop/lile/2012/paper/26">
   <swc:isPartOf rdf:resource="http://data.semanticweb.org/workshop/lile/2012/proceedings"/>
   <dc:creator rdf:resource="http://data.semanticweb.org/person/anastasia-dimou"/>
   <dc:creator rdf:resource="http://data.semanticweb.org/person/charalampos-bratsas"/>
